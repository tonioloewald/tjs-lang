{
  "name": "agent-99",
  "version": "0.0.3",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "CONTEXT.md"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/tonioloewald/agent-99.git"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/jsdom": "^21.1.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "chokidar": "^4.0.3",
    "eslint": "^8.57.1",
    "prettier": "^2.8.8",
    "typescript": "^5.6.2",
    "valibot": "^0.36.0",
    "vitest": "^2.0.5"
  },
  "scripts": {
    "test": "bun test",
    "format": "bun eslint src --fix && bun prettier --write .",
    "lint": "eslint src",
    "make": "bun format && bun typecheck && bun build ./src/index.ts --outdir=dist --minify --sourcemap=external --external @xenova/transformers --external @orama/orama --external jsep --external tosijs-schema && bun run show-size",
    "show-size": "gzip -c dist/index.js | wc -c",
    "typecheck": "tsc --noEmit",
    "latest": "rm -rf node_modules && bun install"
  },
  "dependencies": {
    "jsep": "^1.4.0",
    "tosijs-schema": "1.0.1"
  }
}
