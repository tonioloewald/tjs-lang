{
  "name": "tosijs-agent",
  "version": "0.0.3",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./src": "./src/index.ts"
  },
  "type": "module",
  "files": [
    "dist",
    "CONTEXT.md"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/tonioloewald/tosijs-agent.git"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/jsdom": "^21.1.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "chokidar": "^4.0.3",
    "eslint": "^8.57.1",
    "prettier": "^2.8.8",
    "typescript": "^5.6.2",
    "valibot": "^0.36.0",
    "vitest": "^2.0.5"
  },
  "scripts": {
    "test": "bun test",
    "format": "bun eslint src --fix && bun prettier --write .",
    "lint": "eslint src",
    "make": "rm -rf dist && bun format && tsc -p tsconfig.build.json && bun build ./src/index.ts --outdir=dist --minify --sourcemap=external --external acorn --external tosijs-schema && bun run show-size",
    "show-size": "gzip -c dist/index.js | wc -c | awk '{printf \"%d bytes (%.2f kB)\\n\", $1, $1/1024}'",
    "typecheck": "tsc --noEmit",
    "latest": "rm -rf node_modules && bun install"
  },
  "dependencies": {
    "acorn": "^8.15.0",
    "tosijs-schema": "^1.0.3"
  }
}
